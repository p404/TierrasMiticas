[ITEMDEF i_chest_treasure]
ID=i_chest_metal

ON=@Create
	ATTR=attr_static|attr_move_never
	TYPE=T_CONTAINER_LOCKED
	COLOR=045e
	MORE1=01
	MORE2=01 // the level of the chest.
	TIMER=900

ON=@DCLICK
	if (<MORE1>==02)
		if (<TIMER>>700)
			SRC.MESSAGE=You can't open the chest yet!
			RETURN 1
		else
			TYPE=T_CONTAINER
			ATTR=attr_move_never|attr_decay //activate decay after opening
			TIMER=650
			RETURN 0
		endif
	//***Creating Monster on Opening of Treasure Chest depending on Map Difficulty
	elseif (<MORE1>==01)
		MORE1=02
		TIMER=900
		SOUND=01FE
		IF <MORE2>==01
			SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map random very easy monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map random very easy monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_VEasy //Treasure map random very easy monster spawn
			SRC.ACT.P=<P>
			RETURN 1
		ELSEIF <MORE2>==02
			SRC.NEWITEM=i_Treas_Spawn_Easy //Treasure map random easy monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Easy //Treasure map random easy monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Easy //Treasure map random easy monster spawn
			SRC.ACT.P=<P>
			RETURN 1
		ELSEIF <MORE2>==03
			SRC.NEWITEM=i_Treas_Spawn_Avg //Treasure map random average monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Avg //Treasure map random average monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Avg //Treasure map random average monster spawn
			SRC.ACT.P=<P>
			RETURN 1
		ELSEIF <MORE2>==04
			SRC.NEWITEM=i_Treas_Spawn_Hard //Treasure map random hard monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Hard //Treasure map random hard monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_Hard //Treasure map random hard monster spawn
			SRC.ACT.P=<P>
			RETURN 1
		ELSEIF <MORE2>==05
			SRC.NEWITEM=i_Treas_Spawn_VHard //Treasure map random very hard monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_VHard //Treasure map random very hard monster spawn
			SRC.ACT.P=<P>
			SRC.NEWITEM=i_Treas_Spawn_VHard //Treasure map random very hard monster spawn
			SRC.ACT.P=<P>
			RETURN 1
		ENDIF
		RETURN 0
	endif

